    "@angular/language-service": "~10.0.0-rc.2",
    "@kasaharu/ng-schematics": "^0.3.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "~1.4.2",
    "prettier": "1.19.1",
    "protractor": "~7.0.0",
    "stylelint": "13.2.0",
    "stylelint-config-standard": "20.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.4"
